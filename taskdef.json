{
    "family": "clouds",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "clouds",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 1050,
                    "hostPort": 1050
                }
            ]
        }
    ]
}
